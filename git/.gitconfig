[commit]
	template = ~/.git-commit-template
	gpgsign = true

[core]
	excludesfile = ~/.global.gitignore

[credential]
	helper = osxkeychain

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[gpg]
	program = gpg

[pull]
	rebase = false

